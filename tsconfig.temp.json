{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./",
    "types": [
      "vitest/globals"
    ],
    "resolveJsonModule": true,
    "outDir": "./dist",
    "removeComments": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": false,
    "incremental": true,
    "tsBuildInfoFile": "F:/typescript/typescript-tsd/node_modules/.pnpm/vitest@0.27.1/node_modules/vitest/dist/tsconfig.tmp.tsbuildinfo"
  },
  "include": [
    "packages/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "coverage",
    "docs",
    "./dist"
  ]
}